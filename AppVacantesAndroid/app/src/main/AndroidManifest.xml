<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application

        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppVacantesAndroid"
        tools:targetApi="31"
        tools:ignore="Instantiatable">


        <activity android:name="com.example.vacantesapp.auth.LoginActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.vacantesapp.admin.AdminMainActivity" android:exported="false"/>
        <activity android:name="com.example.vacantesapp.admin.AdminVacantesActivity" android:exported="false"/>
        <activity android:name="com.example.vacantesapp.admin.AdminEmpresasActivity" android:exported="false"/>
        <activity android:name="com.example.vacantesapp.admin.AdminUsuariosActivity" android:exported="false"/>
        <activity android:name="com.example.vacantesapp.user.UserMainActivity" android:exported="false"/>
        <activity android:name="com.example.vacantesapp.user.UserVacantesActivity" android:exported="false"/>
        <activity android:name="com.example.vacantesapp.user.UserSolicitudesActivity" android:exported="false"/>

    </application>
</manifest>